{
    "name": "dota2-lua-annotations",
    "displayName": "Dota2 Lua Annotations",
    "description": "Dota2 lua api annotations for lua-language-server",
    "version": "1.1.4",
    "license": "MIT",
    "publisher": "robincode",
    "icon": "logo.png",
    "keywords": [
        "Lua",
        "Dota2"
    ],
    "repository": {
        "url": "https://github.com/robincodex/dota2-lua-annotations"
    },
    "categories": [
        "Linters",
        "Programming Languages"
    ],
    "extensionDependencies": [
        "sumneko.lua"
    ],
    "engines": {
        "vscode": "^1.77.0"
    },
    "activationEvents": [
        "onLanguage:lua"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "snippets": [
            {
                "language": "lua",
                "path": "./snippets/snippets.code-snippets"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "dump": "ts-node dump/dump.ts"
    },
    "devDependencies": {
        "@moddota/dota-data": "^0.33.0",
        "@types/node": "^18.16.3",
        "@types/vscode": "^1.77.0",
        "axios": "^1.4.0",
        "octokit": "^2.0.14",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.4"
    }
}
