{
    "name": "dota2-lua-annotations",
    "displayName": "Dota2 Lua Annotations",
    "description": "Dota2 lua api annotations for lua-language-server",
    "version": "1.0.3",
    "license": "MIT",
    "publisher": "robincode",
    "icon": "logo.png",
    "keywords": [
        "Lua",
        "Dota2"
    ],
    "repository": {
        "url": "https://github.com/robincodex/dota2-lua-annotations"
    },
    "categories": [
        "Linters",
        "Programming Languages"
    ],
    "extensionDependencies": [
        "sumneko.lua"
    ],
    "engines": {
        "vscode": "^1.76.0"
    },
    "activationEvents": [
        "onLanguage:lua"
    ],
    "main": "./out/extension.js",
    "contributes": {},
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "dump": "ts-node dump/dump.ts"
    },
    "devDependencies": {
        "@moddota/dota-data": "^0.31.1",
        "@types/node": "^18.14.6",
        "@types/vscode": "^1.51.0",
        "axios": "^1.3.4",
        "octokit": "^2.0.14",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    }
}
